    "edge_side_panel": {
        "preferred_width": 380
    },
    "share_target": {
        "action": "./share.html",
        "method": "GET",
        "enctype": "application/x-www-form-urlencoded",
        "params": {
            "title": "title",
            "text": "text",
            "url": "url"
        }
    },
    "protocol_handlers": [
        {
            "protocol": "web+threesides",
            "url": "./protocol.html?url=%s"
        }
    ],
    "file_handlers": [
        {
            "action": "./open-file.html",
            "accept": {
                "text/plain": [
                    ".txt",
                    ".md"
                ],
                "text/markdown": [
                    ".md"
                ]
            },
            "icons": [
                {
                    "src": "icon-192.png",
                    "sizes": "192x192",
                    "type": "image/png"
                }
            ]
        }
    ],
    "widgets": [
        {
            "name": "Quick Note",
            "description": "Start a new journal note",
            "src": "share.html",
            "type": "text/html",
            "sizes": "1x1"
        },
        {
            "name": "Quick Note",
            "description": "Start a new journal note",
            "src": "widgets/quick-note.html",
            "type": "text"
        }
    ],
    "categories": [
        "productivity",
        "utilities",
        "note-taking",
        "health",
        "lifestyle"
    ],
    "launch_handler": {
        "client_mode": "navigate-existing"
    },
    "related_applications": [
        {
            "platform": "play",
            "id": "io.github.cpriss1979.threesides",
            "url": "https://play.google.com/store/apps/details?id=io.github.cpriss1979.threesides"
        }
    ]
}